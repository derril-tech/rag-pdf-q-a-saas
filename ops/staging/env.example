# Created automatically by Cursor AI (2024-12-19)

# Staging Environment Configuration
# Copy this file to .env and fill in your actual values

# Database Configuration
STAGING_DB_HOST=rag-staging-postgres.rag-staging.svc.cluster.local
STAGING_DB_NAME=rag_staging
STAGING_DB_USER=rag_user
STAGING_DB_PASSWORD=your_secure_password_here
STAGING_DB_PORT=5432

# Redis Configuration
STAGING_REDIS_URL=redis://rag-staging-redis.rag-staging.svc.cluster.local:6379
STAGING_REDIS_PASSWORD=your_redis_password_here

# NATS Configuration
STAGING_NATS_URL=nats://rag-staging-nats.rag-staging.svc.cluster.local:4222

# S3/MinIO Configuration
STAGING_S3_BUCKET=rag-staging-documents
STAGING_S3_ACCESS_KEY=your_minio_access_key
STAGING_S3_SECRET_KEY=your_minio_secret_key
STAGING_S3_ENDPOINT=http://rag-staging-minio.rag-staging.svc.cluster.local:9000
STAGING_S3_REGION=us-east-1

# OpenAI Configuration
STAGING_OPENAI_API_KEY=your_openai_api_key_here
STAGING_OPENAI_MODEL=gpt-4-turbo-preview

# Monitoring Configuration
STAGING_SENTRY_DSN=https://your_sentry_dsn_here
STAGING_PROMETHEUS_URL=http://rag-staging-prometheus.rag-staging.svc.cluster.local:9090
STAGING_GRAFANA_URL=http://rag-staging-grafana.rag-staging.svc.cluster.local:3000

# Application Configuration
STAGING_JWT_SECRET=your_jwt_secret_here
STAGING_ENCRYPTION_KEY=your_encryption_key_here
STAGING_SESSION_SECRET=your_session_secret_here

# Slack Integration
STAGING_SLACK_CLIENT_ID=your_slack_client_id
STAGING_SLACK_CLIENT_SECRET=your_slack_client_secret
STAGING_SLACK_SIGNING_SECRET=your_slack_signing_secret

# Stripe Configuration
STAGING_STRIPE_SECRET_KEY=sk_test_your_stripe_test_key
STAGING_STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_test_key
STAGING_STRIPE_WEBHOOK_SECRET=whsec_your_webhook_secret

# Email Configuration
STAGING_SMTP_HOST=smtp.gmail.com
STAGING_SMTP_PORT=587
STAGING_SMTP_USER=your_email@gmail.com
STAGING_SMTP_PASSWORD=your_email_password

# Security Configuration
STAGING_CORS_ORIGINS=https://staging.yourapp.com,https://app.staging.yourapp.com
STAGING_RATE_LIMIT_WINDOW=900000
STAGING_RATE_LIMIT_MAX=100

# Feature Flags
STAGING_ENABLE_HIPAA=false
STAGING_ENABLE_ANALYTICS=true
STAGING_ENABLE_SLACK_INTEGRATION=true
STAGING_ENABLE_STRIPE_BILLING=true

# Resource Limits
STAGING_GATEWAY_REPLICAS=2
STAGING_WORKERS_REPLICAS=3
STAGING_FRONTEND_REPLICAS=2
STAGING_DB_CPU_LIMIT=1000m
STAGING_DB_MEMORY_LIMIT=2Gi
STAGING_REDIS_CPU_LIMIT=500m
STAGING_REDIS_MEMORY_LIMIT=1Gi
